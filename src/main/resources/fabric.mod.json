{
  "schemaVersion": 1,
  "id": "invtemplates",
  "version": "${version}",

  "name": "Inventory Templates",
  "description": "JSON based templates for inventory management.",
  "authors": [
    "HazelTrinity"
  ],
  "contact": {
    "sources": "https://github.com/HazelTheWitch/InvTemplates",
    "issues": "https://github.com/HazelTheWitch/InvTemplates/issues"
  },

  "license": "MIT",
  "icon": "assets/invtemplates/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.hazeltrinity.invtemplates.InvTemplates"
    ],
    "client": [
      "com.hazeltrinity.invtemplates.InvTemplatesClient"
    ]
  },
  "mixins": [
    "invtemplates.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
